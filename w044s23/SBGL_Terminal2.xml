<?xml version="1.0" encoding="UTF-8" ?>
<PropertyList>
 <path>SBGL_Terminal2.ac</path>
  <animation>
  <type>range</type>
  <min-m>0</min-m>
  <max-m>16000</max-m>
 </animation>
  <animation>
  <type>alpha-test</type>
  <alpha-factor>0.01</alpha-factor>
 </animation>
 <animation>
  <type>select</type>
  <object-name>Material1</object-name>
  <condition>
   <greater-than>
    <property>/sim/time/sun-angle-rad</property>
    <value>1.57</value>
   </greater-than>
  </condition>
 </animation>
<animation>
 <type>material</type> 
 <object-name>SBGL_Terminal2_2</object-name>
 <condition>		
  <greater-than>		
   <property>/sim/time/sun-angle-rad</property>
   <value>1.57</value>
  </greater-than>
 </condition>
 <emission>	
  <red>0.25</red>	
  <green>0.25</green>	
  <blue>0.25</blue>
 </emission>
</animation>
</PropertyList>
